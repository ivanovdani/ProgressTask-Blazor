
<ol class="bc root-menu">
    @for (int i = 0; i < Items.Count; i++)
    {
        <MenuItem Model="@Items.ElementAt(i)" Bullet="@($"{i+1}")" CustomCallback="@CustomCallback"></MenuItem>
    }
</ol>

@code {
    [Parameter]
    public List<IMenuItem> Items { get; set; }

    [Parameter]
    public EventCallback<IMenuItem> CustomCallback { get; set; }

}